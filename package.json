{
  "name": "@djabry/grib2json",
  "version": "0.0.1",
  "description": "ecCodes in Lambda",
  "main": "index.js",
  "scripts": {
    "test": "mocha **/*.spec.ts",
    "lint": "tslint **/*.ts",
    "deploy:dev": "sls package --stage=dev",
    "deploy:prod": "sls deploy --stage=prod",
    "docker-build": "docker build . -t grib2json",
    "docker-test": "npm run docker-build && docker run --rm grib2json npm run test",
    "docker-deploy:prod": "npm run docker-test && docker run --rm -e AWS_ACCESS_KEY_ID=$AWS_ACCESS_KEY_ID -e AWS_SECRET_ACCESS_KEY=$AWS_SECRET_ACCESS_KEY grib2json npm run deploy:prod"
  },
  "author": "Daniel Jabry <djabry@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/aws-lambda": "8.10.6",
    "@types/chai": "4.1.3",
    "@types/chai-as-promised": "7.1.0",
    "@types/mocha": "5.2.1",
    "@types/node": "10.3.2",
    "aws-sdk": "2.254.1",
    "brfs": "1.6.1",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "copy-webpack-plugin": "4.5.1",
    "mime": "2.3.1",
    "mocha": "5.2.0",
    "serverless": "1.27.3",
    "serverless-offline": "^3.25.4",
    "serverless-pseudo-parameters": "1.6.0",
    "serverless-webpack": "5.1.5",
    "source-map-support": "0.5.6",
    "transform-loader": "0.2.4",
    "ts-loader": "4.4.1",
    "ts-node": "6.1.0",
    "ts-stream": "1.0.1",
    "tslint": "5.10.0",
    "tslint-loader": "3.6.0",
    "typescript": "2.9.1",
    "webpack": "4.12.0",
    "webpack-cli": "3.0.3",
    "winston": "^3.0.0-rc6"
  }
}
